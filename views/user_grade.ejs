<% include _head.ejs %>
<% include _header.ejs %>
<% include _nav.ejs %>

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-8 col-sm-offset-2">
            <table class="table table-striped table-bordered">
                <h4>共 <%= grade.length %> 门课程</h4>
                <tr>
                    <th>课程名</th>
                    <th>课程号</th>
                    <th>课序号</th>
                    <th>学分</th>
                    <th>课程属性</th>
                    <th>成绩</th>
                </tr>
                <% grade.forEach(function (item) { %>
                <tr>
                    <td><%= item.courseName %></td>
                    <td><%= item.courseNumber %></td>
                    <td><%= item.lessonNumber %></td>
                    <td><%= item.credit %></td>
                    <td><%= item.courseProperty %></td>
                    <td><%= item.grade %></td>
                </tr>
                <% }); %>
            </table>
        </div>
    </div>
</div>

<% include _scripts.ejs %>
<% include _footer.ejs %>
